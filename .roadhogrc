{
  "entry": ["babel-polyfill", "src/index.js"],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        "transform-decorators-legacy"
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        "transform-decorators-legacy"
      ]
    }
  },
  "sass": true,
  "disableCSSModules": true,
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "theme": {
    "primary-color": "#ff9900"
  },
  "publicPath": "./",
  "hash": true,
  "manifest": {
    "basePath": "/src/"
  }
}
